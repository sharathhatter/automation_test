<resources xmlns:android="http://schemas.android.com/apk/res/android">

      <style name="SDKTheme" parent="android:Theme.Light" />

    <style name="LightErrorFix" parent="SDKTheme">
        <item name="android:textColorSecondaryInverse">@android:color/secondary_text_light</item>
    </style>

    <style name="Theme.Transparent" parent="LightErrorFix">
        <item name="android:windowIsTranslucent">true</item>
        <item name="android:windowBackground">@android:color/transparent</item>
        <item name="android:windowContentOverlay">@null</item>
        <item name="android:windowNoTitle">true</item>
        <item name="android:windowIsFloating">true</item>
        <item name="android:backgroundDimEnabled">true</item>
    </style>


    <style name="footer">
        <item name="android:orientation">vertical</item>
        <item name="android:layout_marginLeft">16dp</item>
        <item name="android:layout_marginRight">16dp</item>
        <item name="android:layout_marginTop">8dp</item>
        <item name="android:layout_width">fill_parent</item>
        <!-- <item name="android:layout_height">wrap_content</item> -->
    </style>

    <!-- New -->

    <style name="new_layout_container">
        <item name="android:orientation">vertical</item>
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_weight">1</item>
        <item name="android:paddingLeft">@dimen/center_container_padding_left</item>
        <item name="android:paddingRight">@dimen/center_container_padding_right</item>
        <item name="android:paddingTop">@dimen/center_container_padding_top</item>
        <item name="android:paddingBottom">@dimen/center_container_padding_bottom</item>
        <item name="android:layout_height">fill_parent</item>
    </style>

    <style name="body_new">
        <item name="android:layout_width">fill_parent</item>
        <item name="android:background">@color/uic_bg</item>
        <item name="android:paddingLeft">12dp</item>
        <item name="android:paddingRight">12dp</item>
        <item name="android:fillViewport">true</item>
        <item name="android:layout_height">fill_parent</item>
    </style>

    <style name="text" parent="android:style/Widget.TextView">

        <!-- <item name="android:background">#ffffff</item> -->
        <!-- <item name="android:padding">2dp</item> -->
        <!-- <item name="android:layout_margin">1dp</item> -->
        <item name="android:layout_width">fill_parent</item>
        <item name="android:textSize">16sp</item>
        <item name="android:layout_height">wrap_content</item>
        <!-- <item name="android:textAppearance">?android:attr/textAppearanceMedium</item> -->
        <item name="android:textColor">@color/text_color</item>
    </style>

    <style name="text_tableCell" parent="@style/text">
        <item name="android:paddingLeft">4dp</item>
        <item name="android:paddingTop">16dp</item>
        <item name="android:paddingRight">4dp</item>
        <item name="android:paddingBottom">16dp</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
         <item name="android:textAppearance">?android:attr/textAppearanceLarge</item>
        <item name="android:gravity">center</item>
    </style>

    <style name="new_button" parent="android:style/Widget.Button">
        <item name="android:textColor">#FFF</item>
        <item name="android:textStyle">bold</item>
        <item name="android:textSize">18sp</item>
        <!-- <item name="android:textAppearance">?android:attr/textAppearanceMediumInverse</item> -->
        <item name="android:layout_marginTop">@dimen/button_margin_top</item>
        <item name="android:background">@color/button_background_new</item>
        <item name="android:paddingLeft">32dp</item>
        <item name="android:paddingRight">32dp</item>
        <item name="android:minHeight">40dip</item>
        <item name="android:minWidth">64dip</item>
        <item name="android:paddingTop">10dp</item>
        <item name="android:paddingBottom">10dp</item>
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_gravity">center_horizontal</item>
    </style>

     <style name="new_secondary_button" parent="android:style/Widget.Button">
        <item name="android:textColor">@color/secondary_btn_text_selector</item>
        <item name="android:textStyle">bold</item>
        <item name="android:textSize">18sp</item>
        <!-- <item name="android:textAppearance">?android:attr/textAppearanceMediumInverse</item> -->
        <item name="android:layout_marginTop">@dimen/button_margin_top</item>
        <item name="android:background">@null</item>
        <item name="android:paddingLeft">32dp</item>
        <item name="android:paddingRight">32dp</item>
        <item name="android:minHeight">40dip</item>
        <item name="android:minWidth">64dip</item>
        <item name="android:paddingTop">10dp</item>
        <item name="android:paddingBottom">10dp</item>
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_gravity">center_horizontal</item>
    </style>
    
    <style name="full_layout">
        <item name="android:orientation">vertical</item>
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">wrap_content</item>
    </style>
    
    
    <style name="wallet_textbox" parent="android:style/Widget.EditText">
        <item name="android:layout_marginTop">0dp</item>
        <item name="android:layout_width">fill_parent</item>
        <item name="android:background">@drawable/edit_text_bg</item>
        <item name="android:layout_height">wrap_content</item>
    </style>

    <style name="wallet_textbox_cvv" parent="android:style/Widget.EditText">
        <item name="android:layout_marginTop">0dp</item>
        <item name="android:layout_width">fill_parent</item>
        <item name="android:background">@drawable/cvv_text_bg</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:inputType">numberPassword</item>
    </style>


    <style name="textBlueHeading">

        <!-- <item name="android:textColor">#fff</item> -->
        <item name="android:textSize">17sp</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_gravity">center</item>
        <item name="android:gravity">center</item>
        <item name="android:textColor">@color/uic_header_bg</item>
        <item name="android:textStyle">bold</item>
    </style>


    <style name="white_container">
        <item name="android:background">@drawable/s_container</item>
        <item name="android:paddingLeft">12dp</item>
        <item name="android:paddingRight">12dp</item>
        <item name="android:paddingTop">12dp</item>
        <item name="android:paddingBottom">12dp</item>
        <!-- <item name="android:layout_marginTop">@dimen/fields_margin_top</item> -->
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_width">fill_parent</item>
    </style>

    <style name="main_container">
        <item name="android:background">@color/uic_bg</item>
        <item name="android:paddingLeft">12dp</item>
        <item name="android:paddingRight">12dp</item>
        <item name="android:layout_height">match_parent</item>
        <item name="android:layout_width">match_parent</item>
    </style>

</resources>