            android:name="com.paytm.pgsdk.PaytmPGActivity"
            android:configChanges="keyboardHidden|orientation|keyboard"
            android:screenOrientation="portrait" />

        <!-- MOEngage -->
        <receiver
            android:name="com.moe.pushlibrary.PushGcmBroadcastReceiver"
            android:permission="com.google.android.c2dm.permission.SEND">
            <intent-filter>
                <action android:name="com.google.android.c2dm.intent.RECEIVE" />
                <action android:name="com.google.android.c2dm.intent.REGISTRATION" />

                <category android:name="${applicationId}" />
            </intent-filter>
        </receiver>

        <!-- MOENGAGE RECEIVER FOR RECEIVING INSTALLATION INTENT -->
        <receiver android:name="com.bigbasket.mobileapp.receivers.AppUpdateReceiver">
            <intent-filter>
                <action android:name="android.intent.action.PACKAGE_REPLACED" />

                <data
                    android:path="${applicationId}"
                    android:scheme="package" />
            </intent-filter>
        </receiver>

        <provider
            android:name="com.moe.pushlibrary.providers.MoEProvider"
            android:authorities="${applicationId}.moengage.provider"
            android:exported="false" />

        <!-- push notification landing page Need to change parent Activity -->
        <activity
            android:name="com.moe.pushlibrary.activities.MoEActivity"
            android:parentActivityName="com.bigbasket.mobileapp.activity.SplashActivity"
            android:screenOrientation="portrait">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.bigbasket.mobileapp.activity.SplashActivity" />
        </activity>

        <!-- MANDATORY FIELD: APP ID AS SEEN ON MOENGAGE DASHBOARD APP SETTINGS PAGE -->
        <meta-data
            android:name="APP_ID"
            android:value="@string/moe_app_id" />

        <!-- MANDATORY FIELD: SENDER ID , i.e. THE PROJECT NUMBER AS MENTIONED ON GOOGLE CLOUD CONSOLE PROJECTS PAGE -->
        <meta-data
            android:name="SENDER_ID"
            android:value="@string/moe_gcm_sender_id" />

        <!-- MANDATORY FIELD: THE NOTIFICATION ICON WHICH WILL BE USED TO SET TO NOTIFICATIONS POSTED -->
        <meta-data
            android:name="NOTIFICATION_ICON"
            android:value="notification_icon" />
        <meta-data
            android:name="NOTIFICATION_LARGE_ICON"
            android:value="notification_icon_large" />

        <!-- moEngage UBox -->
        <activity
            android:name="com.bigbasket.mobileapp.activity.CommunicationHubActivity"
            android:label="@string/title_activity_communication_hub"
            android:parentActivityName="com.bigbasket.mobileapp.activity.SplashActivity"
            android:theme="@style/BackButtonTheme"
            android:windowSoftInputMode="adjustResize">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.bigbasket.mobileapp.activity.SplashActivity" />
        </activity>

        <service
            android:name="com.moengage.addon.ubox.UBoxMessenger"
            android:exported="false" />

        <service
            android:name="com.bigbasket.mobileapp.service.BBMessenger"
            android:exported="false" />

        <meta-data
            android:name="LOCALYTICS_APP_KEY"
            android:value="@string/localytics_api_key" />
        <meta-data
            android:name="io.fabric.ApiKey"
            android:value="${crashlytics_id}" />
        <meta-data
            android:name="com.google.android.gms.version"
            android:value="@integer/google_play_services_version" />
        <meta-data
            android:name="com.google.android.geo.API_KEY"
            android:value="@string/google_geo_api_key" />

        <service
            android:name="com.bigbasket.mobileapp.service.AreaPinInfoIntentService"
            android:exported="false" />
        <service
            android:name="com.bigbasket.mobileapp.service.GetAppDataDynamicIntentService"
            android:exported="false" />

        <receiver
            android:name="com.bigbasket.mobileapp.receivers.InstallReferrerReceiver"
            android:exported="true">
            <intent-filter>
                <action android:name="com.android.vending.INSTALL_REFERRER" />
            </intent-filter>
        </receiver>

        <provider
            android:name="com.bigbasket.mobileapp.adapter.db.DatabaseContentProvider"
            android:authorities="${applicationId}.bbprovider"
            android:enabled="true"
            android:exported="false" />

        <service
            android:name="com.bigbasket.mobileapp.service.HomePageSyncService"
            android:exported="false" />
        <service
            android:name="com.bigbasket.mobileapp.service.MainMenuSyncService"
            android:exported="false" />

        <receiver
            android:name="com.google.android.gms.analytics.AnalyticsReceiver"
            android:enabled="true">
            <intent-filter>
                <action android:name="com.google.android.gms.analytics.ANALYTICS_DISPATCH" />
            </intent-filter>
        </receiver>

        <service
            android:name="com.google.android.gms.analytics.AnalyticsService"
            android:enabled="true"
            android:exported="false" />
        <service android:name="com.google.android.gms.analytics.CampaignTrackingService"
            android:enabled="true"
            android:exported="false" />
        <service
            android:name="com.bigbasket.mobileapp.service.AnalyticsIntentService"
            android:exported="false" />
    </application>

</manifest>
